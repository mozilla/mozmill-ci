{
    "pulse": {
        "applabel": "qa-auto@mozilla.com|mozmill_release",
        "branches": [
            "release-mozilla-beta",
            "release-mozilla-release",
            "release-mozilla-esr31",
            "release-mozilla-esr24"
        ],
        "platforms": [
            "linux",
            "linux64",
            "macosx",
            "macosx64",
            "win32"
        ],
        "products": [
            "firefox"
        ],
        "tags": [
        ]
    },
    "jenkins": {
        "url": "http://localhost:8080",
        "username": "mozilla",
        "password": "test1234"
    },
    "testrun": {
        "jenkins_parameter_map": {
            "default": {
                "APPLICATION": {
                    "key": "product"
                },
                "BRANCH": {
                    "key": "branch"
                },
                "BUILD_NUMBER": {
                    "key": "build_number"
                },
                "BUILD_TYPE": {
                    "value": "candidate"
                },
                "LOCALE": {
                    "key": "locale"
                },
                "PLATFORM": {
                    "key": "platform",
                    "transform": "get_platform_identifier"
                },
                "REPORT_URL": {
                    "value": "http://mozauto.iriscouch.com/mozmill-release/"
                },
                "VERSION": {
                    "key": "version"
                }
            },
            "endurance": {
                "ENTITIES": {
                    "value": 10
                },
                "ITERATIONS": {
                    "value": 10
                }
            },
            "update": {
                "BUILD_ID": {
                    "key": "previous_buildid"
                },
                "TARGET_BUILD_ID": {
                    "key": "buildid"
                }
            }
        },
        "by_branch": {
            "release-mozilla-beta": {
                "blacklist": {
                    "locales": [
                        "az",
                        "ku"
                    ]
                },
                "testruns": [
                    "functional",
                    "remote"
                ],
                "platforms": {
                    "linux": [
                        "linux && ubuntu && 12.04 && 32bit",
                        "linux && ubuntu && 13.10 && 32bit"
                    ],
                    "linux64": [
                        "linux && ubuntu && 12.04 && 64bit",
                        "linux && ubuntu && 13.10 && 64bit"
                    ],
                    "mac": [
                        "mac && 10.6 && 64bit",
                        "mac && 10.7 && 64bit",
                        "mac && 10.8 && 64bit",
                        "mac && 10.9 && 64bit"
                    ],
                    "win32": [
                        "windows && xp && 32bit",
                        "windows && vista && 32bit",
                        "windows && 7 && 32bit",
                        "windows && 7 && 64bit",
                        "windows && 8 && 32bit",
                        "windows && 8 && 64bit",
                        "windows && 8.1 && 32bit",
                        "windows && 8.1 && 64bit"
                    ]
                }
            },
